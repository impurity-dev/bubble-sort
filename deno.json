{
    "name": "@collections/all",
    "version": "0.0.1",
    "license": "MIT",
    "exports": "./mod.ts",
    "tasks": {
        "setup": "deno run -A setup.ts",
        "dev": "deno run --watch main.ts",
        "test": "deno test --coverage=./coverage"
    },
    "test": {
        "exclude": ["**/test-suite.test.ts"]
    },
    "imports": {
        "@algorithm/": "./src/algorithm/",
        "@graph/": "./src/graph/",
        "@linked-list/": "./src/linked-list/",
        "@queue/": "./src/queue/",
        "@shared/": "./src/shared/",
        "@stack/": "./src/stack/",
        "@tree/": "./src/tree/",
        "@std/assert": "jsr:@std/assert@1",
        "fast-check": "npm:fast-check@^4.2.0"
    },
    "fmt": {
        "lineWidth": 180,
        "semiColons": true,
        "singleQuote": true,
        "indentWidth": 4
    }
}
